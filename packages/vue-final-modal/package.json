{
  "name": "vue-final-modal",
  "private": false,
  "version": "4.0.0-rc.0",
  "source": "src/index.ts",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vue-tsc --noEmit && vite build -w",
    "build": "vue-tsc --noEmit && vite build && vue-tsc -p tsconfig.build-dts.json --declaration --emitDeclarationOnly",
    "dev:viteplay": "vite --config vite.config.play.ts --host",
    "build:viteplay": "vite build --config vite.config.play.ts",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "typecheck": "vue-tsc --noEmit",
    "release": "pnpm build && pnpm test && release-it",
    "postinstall": "vue-tsc --noEmit && vite build"
  },
  "dependencies": {
    "@vueuse/core": "^9.4.0",
    "@vueuse/integrations": "^9.4.0",
    "focus-trap": "^7.0.0",
    "vue": "^3.2.41",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^5.1.1",
    "@viteplay/plugin": "^0.2.4",
    "@viteplay/vue": "^0.2.4",
    "@vitest/coverage-istanbul": "^0.24.5",
    "jsdom": "^20.0.2",
    "release-it": "^15.5.0",
    "unplugin-vue-macros": "^0.15.2",
    "vitest": "^0.24.5"
  },
  "peerDependencies": {
    "@vueuse/core": "^9.4.0",
    "@vueuse/integrations": "^9.4.0",
    "focus-trap": "^7.0.0"
  }
}
